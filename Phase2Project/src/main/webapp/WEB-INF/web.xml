<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>PageController</servlet-name>
  	<display-name>PageController</display-name>
  	<description></description>
  	<servlet-class>vikas.RatanRaman.PageController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>vikas.RatanRaman.User.UserController</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>PageController</servlet-name>
  	<url-pattern>/PageController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/UserPage</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>FlightController</servlet-name>
  <servlet-class>vikas.RatanRaman.Flights.FlightController</servlet-class>
  <load-on-startup>2</load-on-startup>
  
  </servlet>
  <servlet-mapping>
  	<servlet-name>FlightController</servlet-name>
  	<url-pattern>/FlightPage</url-pattern>
  </servlet-mapping>
</web-app>
